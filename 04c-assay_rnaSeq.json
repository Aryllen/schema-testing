{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"nkauer-ad.assay.rnaSeq",
    "description": "SysBio AD rnaSeq annotations schema",
    "allOf": [
        {
            "$ref": "nkauer-assay.rnaSeq"
        },
        {
            "properties": {
                "dataType": {
                    "enum": [ "geneExpression" ]
                },
                "dataSubtype": {
                    "$ref": "sage.annotations-experimentalData.dataSubtype"
                }
            }
        },
        {
            "required": [
                "platform",
                "libraryPrep",
                "readLength",
                "runType",
                "dataType",
                "dataSubtype"
            ]
        }
    ]
}
