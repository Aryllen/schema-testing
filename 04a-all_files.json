{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"nkauer-ad.all.files",
    "description": "SysBio AD required annotations schema for all files",
    "allOf": [
        {
            "properties": {
                "fileFormat": {
                    "$ref": "sage.annotations-sageCommunity.fileFormat"
                },
                "consortium": {
                    "$ref": "sage.annotations-sageCommunity.consortium"
                }
            },
            "required": [
                "fileFormat",
                "consortium"
            ]
        }
    ]
}
